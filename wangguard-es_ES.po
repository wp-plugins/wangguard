#: ../wangguard-admin.php:422
#: ../wangguard-admin.php:1516
msgid "Question"
msgstr "Pregunta"

#: ../wangguard-admin.php:413
#: ../wangguard-admin.php:424
#: ../wangguard-admin.php:1517
msgid "Answer"
msgstr "Respuesta"

#: ../wangguard-admin.php:414
